                Name="ScriptMain.vb"
                Encoding="UTF8"><![CDATA[' Microsoft SQL Server Integration Services user script component
' This is your new script component in Microsoft Visual Basic .NET
' ScriptMain is the entrypoint class for script components
Option Strict Off

Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime
Imports System.Net
Imports System.Xml
Imports System.IO
Imports System.Text


<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
	Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
		Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
	End Enum

	' The execution engine calls this method when the task executes.
	' To access the object model, use the Dts object. Connections, variables, events,
	' and logging features are available as static members of the Dts class.
	' Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
	' 
	' To open Code and Text Editor Help, press F1.
	' To open Object Browser, press Ctrl+Alt+J.

    Public Sub Main()
        Dim readStream As StreamReader
        Dim AuthorXML As String = Dts.Variables("Authors").Value.ToString.Replace("#", "")
        Dim PmidXml As String = ""
        Dim url As String = Dts.Variables("HMSExporterWebService").Value.ToString
        Dim response As HttpWebResponse
        Try
            Dim bytes As Byte() = Encoding.UTF8.GetBytes(AuthorXML)
            Dim request As HttpWebRequest = DirectCast(WebRequest.Create(url), HttpWebRequest)

            ' Set Web Request Properties
            request.Method = "POST"
            request.ContentLength = bytes.Length
            request.ContentType = "text/xml"
            request.Timeout = 330000

            ' Read in AuthorXML
            Using requestStream As Stream = request.GetRequestStream()
                requestStream.Write(bytes, 0, bytes.Length)
            End Using

            ' Post XML to HMS WebService
            response = DirectCast(request.GetResponse(), HttpWebResponse)
            If response.StatusCode <> HttpStatusCode.OK Then
                Dim message As String = [String].Format("POST failed. Received HTTP {0}", response.StatusCode)
                Throw New ApplicationException(message)
            End If

            ' Read Response into String
            Dim rs As HttpWebResponse = DirectCast(request.GetResponse(), HttpWebResponse)
            Dim receiveStream As Stream = rs.GetResponseStream()
            Dim encode As Encoding = System.Text.Encoding.GetEncoding("utf-8")
            readStream = New StreamReader(receiveStream, encode)
            Dim read(25000) As [Char]
            Dim count As Integer = readStream.Read(read, 0, 25000)
            While count > 0
                Dim str As New [String](read, 0, count)
                PmidXml += str
                count = readStream.Read(read, 0, 25000)
            End While
            readStream.Close()
            rs.Close()

            ' Read PMID XML into list 
            Dts.Variables("DisambiguationResults").Value = PmidXml

            ' Release the resources of stream object.
            readStream.Close()

            ' Release the resources of response object.
            response.Close()
            Dts.TaskResult = ScriptResults.Success
        Catch ex As Exception
            Console.WriteLine(ex.ToString)
            Dts.Variables("ErrorText").Value = Dts.Variables("ErrorText").Value + ex.ToString
            Dts.TaskResult = ScriptResults.Failure
        Finally

        End Try
    End Sub

End Class]]></ProjectItem>
